{
    "name": "neeckeloo/newrelic",
    "description": "NewRelic module for Laminas",
    "type": "library",
    "keywords": [
        "laminas", "newrelic"
    ],
    "license": "MIT",
    "homepage": "https://github.com/neeckeloo/NewRelic",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 | ^8.0",
        "psr/container": "1.0",
        "monolog/monolog": "^1.17 || ^2.0",
        "laminas/laminas-console": "^2.6",
        "laminas/laminas-eventmanager": "^2.6 || ^3.0",
        "laminas/laminas-servicemanager": "^2.7 || ^3.0",
        "laminas/laminas-http": "^2.5",
        "laminas/laminas-mvc": "^2.7 || ^3.0",
        "laminas/laminas-stdlib": "^3.2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8",
        "php-coveralls/php-coveralls": "^2.0",
        "phpspec/prophecy": "^1.9"
    },
    "conflict": {
        "container-interop/container-interop": "<1.2"
    },
    "autoload": {
        "psr-4": {
            "NewRelic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NewRelicTest\\": "tests/"
        }
    }
}
